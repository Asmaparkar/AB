Create table emp_track(emp_no int,dept text,name text,PRIMARY KEY(dept,emp_no))
WITH Clustering ORDER BY(emp_no desc);
Insert into emp_track(emp_no,dept,name) values(123,’database’,’virat’);
Select * from emp_track;
